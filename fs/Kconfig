
menu "Filesystem support            "

config FS_AUTOMOUNT
	bool

config FS_CRAMFS
	bool
	select ZLIB
	prompt "cramfs support"

config FS_RAMFS
	bool
	default y
	prompt "ramfs support"

config FS_DEVFS
	bool
	default y
	prompt "devfs support"

config FS_TFTP
	bool
	prompt "tftp support"
	depends on NET

config FS_NFS
	depends on NET
	bool
	prompt "nfs support"

source fs/fat/Kconfig

config PARTITION_NEED_MTD
	bool

endmenu
